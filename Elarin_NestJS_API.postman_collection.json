{
  "info": {
    "name": "Elarin NestJS API",
    "description": "Coleção completa da API Elarin (NestJS + Fastify)",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3001",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    var jsonData = pm.response.json();",
                  "    if (jsonData.session && jsonData.session.access_token) {",
                  "        pm.environment.set(\"token\", jsonData.session.access_token);",
                  "        pm.environment.set(\"userId\", jsonData.user.id);",
                  "        console.log(\"Token saved: \" + jsonData.session.access_token);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"{{testEmail}}\",\n  \"password\": \"{{testPassword}}\",\n  \"full_name\": \"Usuario Teste\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            },
            "description": "Registrar novo usuário. Salva o token automaticamente."
          },
          "response": []
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.environment.set(\"token\", jsonData.session.access_token);",
                  "    pm.environment.set(\"userId\", jsonData.user.id);",
                  "    console.log(\"Token saved: \" + jsonData.session.access_token);",
                  "    pm.test(\"Token received\", function () {",
                  "        pm.expect(jsonData.session.access_token).to.be.a('string');",
                  "    });",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"{{testEmail}}\",\n  \"password\": \"{{testPassword}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            },
            "description": "Fazer login. Salva o token automaticamente nas variáveis de ambiente."
          },
          "response": []
        },
        {
          "name": "Logout",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{token}}",
                  "type": "string"
                }
              ]
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/auth/logout",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "logout"]
            },
            "description": "Fazer logout (requer token)"
          },
          "response": []
        },
        {
          "name": "Test Validation - Invalid Email",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"email-invalido\",\n  \"password\": \"123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            },
            "description": "Testar validação de DTOs com dados inválidos"
          },
          "response": []
        }
      ],
      "description": "Endpoints de autenticação"
    },
    {
      "name": "Exercises",
      "item": [
        {
          "name": "Get All Exercises",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/exercises",
              "host": ["{{baseUrl}}"],
              "path": ["exercises"]
            },
            "description": "Listar todos os exercícios ativos (requer autenticação)"
          },
          "response": []
        },
        {
          "name": "Get Exercise by Type",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/exercises/:type",
              "host": ["{{baseUrl}}"],
              "path": ["exercises", ":type"],
              "variable": [
                {
                  "key": "type",
                  "value": "push_up",
                  "description": "Tipo do exercício (ex: push_up, squat)"
                }
              ]
            },
            "description": "Buscar exercício específico por tipo (requer autenticação)"
          },
          "response": []
        },
        {
          "name": "Test Auth - No Token",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/exercises",
              "host": ["{{baseUrl}}"],
              "path": ["exercises"]
            },
            "description": "Testar endpoint protegido sem token (deve retornar 401)"
          },
          "response": []
        }
      ],
      "description": "Endpoints de exercícios (todos requerem autenticação)"
    },
    {
      "name": "Training",
      "item": [
        {
          "name": "Create Training Session",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.environment.set(\"sessionId\", jsonData.id);",
                  "    console.log(\"Session ID saved: \" + jsonData.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{token}}",
                  "type": "string"
                }
              ]
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"exercise_type\": \"push_up\",\n  \"target_reps\": 10,\n  \"target_sets\": 3\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/training/sessions",
              "host": ["{{baseUrl}}"],
              "path": ["training", "sessions"]
            },
            "description": "Criar nova sessão de treino. Salva o session_id automaticamente."
          },
          "response": []
        },
        {
          "name": "Complete Training Session",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{token}}",
                  "type": "string"
                }
              ]
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": \"{{sessionId}}\",\n  \"reps_completed\": 10,\n  \"sets_completed\": 3,\n  \"duration_seconds\": 300,\n  \"avg_confidence\": 0.95\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/training/sessions/complete",
              "host": ["{{baseUrl}}"],
              "path": ["training", "sessions", "complete"]
            },
            "description": "Completar sessão de treino (usa o sessionId salvo automaticamente)"
          },
          "response": []
        },
        {
          "name": "Get Training History",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/training/history?limit=20&offset=0",
              "host": ["{{baseUrl}}"],
              "path": ["training", "history"],
              "query": [
                {
                  "key": "limit",
                  "value": "20",
                  "description": "Número de registros (padrão: 20)"
                },
                {
                  "key": "offset",
                  "value": "0",
                  "description": "Offset para paginação (padrão: 0)"
                }
              ]
            },
            "description": "Ver histórico de treinos do usuário"
          },
          "response": []
        },
        {
          "name": "Get Session Details",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/training/sessions/:id",
              "host": ["{{baseUrl}}"],
              "path": ["training", "sessions", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "{{sessionId}}",
                  "description": "UUID da sessão"
                }
              ]
            },
            "description": "Ver detalhes de uma sessão específica (usa o sessionId salvo)"
          },
          "response": []
        }
      ],
      "description": "Endpoints de treino (todos requerem autenticação)"
    },
    {
      "name": "Health Check",
      "item": [
        {
          "name": "Swagger Documentation",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/docs",
              "host": ["{{baseUrl}}"],
              "path": ["docs"]
            },
            "description": "Acessar documentação Swagger"
          },
          "response": []
        }
      ],
      "description": "Endpoints utilitários"
    }
  ]
}
